  "dependencies": {
    "http-errors": "^1.8.0",
    "isobject": "^4.0.0"
  },
  "devDependencies": {
    "coverage-node": "^5.0.1",
    "eslint": "^7.32.0",
    "eslint-config-env": "^22.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^15.0.0",
    "jsdoc-md": "^11.0.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "node-fetch": "^3.0.0",
    "prettier": "^2.4.1",
    "test-director": "^7.0.0"
  },
  "scripts": {
