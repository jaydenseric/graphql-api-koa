{
  "name": "graphql-api-koa",
  "version": "6.0.0",
  "description": "GraphQL execution and error handling middleware written from scratch for Koa.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/graphql-api-koa",
  "homepage": "https://github.com/jaydenseric/graphql-api-koa#readme",
  "bugs": "https://github.com/jaydenseric/graphql-api-koa/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "graphql",
    "api",
    "koa",
    "esm",
    "mjs"
  ],
  "files": [
    "private",
    "public"
  ],
  "main": "./public/index.mjs",
  "exports": {
    ".": "./public/index.mjs",
    "./public/*": "./public/*",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.20 || >= 14.13"
  },
  "peerDependencies": {
    "graphql": "0.13.1 - 15"
  },
  "dependencies": {
    "http-errors": "^1.8.0",
    "isobject": "^4.0.0"
  },
  "devDependencies": {
    "coverage-node": "^5.0.1",
    "eslint": "^7.25.0",
    "eslint-config-env": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^33.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "graphql": "^15.0.0",
    "jsdoc-md": "^10.1.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "test-director": "^6.0.0"
  },
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier --write readme.md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "test:api": "coverage-node test/index.mjs",
    "prepublishOnly": "npm test"
  }
}
